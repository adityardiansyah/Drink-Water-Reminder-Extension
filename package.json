{
  "name": "waktunya-minum",
  "displayName": "Waktunya Minum",
  "description": "Pengingat Minum kamu saat ngoding",
  "publisher": "AdityaArdiansyah",
  "version": "0.0.3",
  "license": "MIT",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "water",
    "drink reminder",
    "drink water",
    "drink water reminder",
    "waktunya minum",
    "minum"
  ],
  "icon": "logo.png",
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "waktunya-minum.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "waktunya-minum.setInterval",
        "title": "Tetapkan Jeda Minum Air"
      },
      {
        "command": "waktunya-minum.remindNow",
        "title": "ðŸ’§ Ingatkan untuk Minum Sekarang"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Config Waktunya Minum",
      "properties": {
        "waktunya-minum.intervalMinutes": {
          "type": "number",
          "default": 30,
          "minimum": 1,
          "description": "Interval pengingat dalam menit untuk minum air."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/sinon": "^21.0.0",
    "@types/vscode": "^1.107.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.1",
    "sinon": "^21.0.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adityardiansyah/Drink-Water-Reminder-Extension.git"
  }
}
